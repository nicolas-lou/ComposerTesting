{
    "name": "aston/cart",
  "version": "1.0.0",
    "description": "Shoppring cart",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "nicolas lou",
            "email": "nicolas.louadoudi@gmail.com"
        }
    ],
    "scripts": {
        "server": "php -S localhost:3000/testing -docroot &",
      "clean": ["phpcbf --standard=PSR2 ./src"],
      "test": [
        "phpcs --standard=PSR2 ./src",
        "phpunit --colors=always --configuration phpUnit.xml"
      ]
    },
    "autoload": {
        "psr-4": {
            "Aston\\": "src/Aston"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Aston\\": "test/Aston"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [{
    "type": "git",
    "url": "https://github.com/nicolas-lou/ComposerTesting"
  }],
    "require": {
      "php": ">=7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
      "phpunit/php-code-coverage": "^6.1@dev",
      "squizlabs/php_codesniffer": "3.0@dev"
    }
}
